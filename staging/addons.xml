<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.program.xsqueezechooser" name="XSqueeze Chooser"
       version="0.2.2" provider-name="bossanova808">
  <requires>
    <import addon="xbmc.python" version="2.14.0"/>
    <import addon="script.module.xsqueezecommon" version="0.2.3"/>
    <import addon="script.module.b808common" version="0.2.1"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>Installed automatically with XSqueeze, this Chooser allows you to select music to play on your XSqueeze player.</summary>
    <description>Installed automatically with XSqueeze, this Chooser allows you to select music to play on your XSqueeze player.</description>
    <platform>all</platform>
    <license>GPL V2</license>
  </extension>
</addon>

<addon
  id="repository.bossanova808"
  name="bossanova808's XBMC Addons"
  version="0.2.0"
  provider-name="bossanova808">
	<extension point="xbmc.addon.repository"
             name="bossanova808's XBMC Addon Repository">
    <info compressed="false">https://raw.github.com/bossanova808/repository.bossanova808/master/staging/addons.xml</info>
    <checksum>https://raw.github.com/bossanova808/repository.bossanova808/master/staging/addons.xml.md5</checksum>
    <datadir zip="true">https://raw.github.com/bossanova808/repository.bossanova808/master/repository-downloads/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Repo for Bossanova808's XBMC Addons</summary>
		<description>Download and install addons by bossanova808, including XSqueeze.</description>
		<platform>all</platform>
	</extension>
</addon>

<addon id="script.kodi.yoctodisplay" name="Kodi YoctoDisplay" version="0.0.3" provider-name="bossanova808">
  <requires>
    <import addon="xbmc.python" version="2.24.0"/>
    <import addon="xbmc.addon" version="12.0.0"/>
  </requires>
  <extension point="xbmc.service" library="default.py" start="startup"></extension>
  <extension point="xbmc.addon.metadata">
    <language></language>
    <summary>Kodi YoctoDispay</summary>
    <description>Display current time, current temperatre and media playing time remaining on a YoctoMaxiDisplay screen.</description>
    <disclaimer>Works only with Yoctopuce USB MaxiDisplay.  Shouldn't blow up the universe or any part of it, but use at your own risk.</disclaimer>
    <platform>all</platform>
    <language>en</language>
    <license>GNU GENERAL PUBLIC LICENSE. Version 2, June 1991</license>
    <forum></forum>
    <website></website>
    <email></email>
    <source>https://github.com/bossanova808/script.kodi.yoctodisplay</source>
  </extension>
</addon>

<addon id="script.module.b808common" version="0.2.3" name="Bossanova808 Common Code" provider-name="bossanova808">
  <requires>
    <import addon='xbmc.python' version='2.14.0'/>
  </requires>
  <extension point='xbmc.addon.metadata'>
    <platform>all</platform>
    <summary lang='en'>Installed automatically with Bossanova808 addons - common code for XSqueeze etc.</summary>
  </extension>
  <extension point='xbmc.python.module' library='lib' />
</addon>

<addon id="script.module.xsqueezecommon" version="0.2.3" name="XSqueeze Common Code" provider-name="bossanova808">
  <requires>
    <import addon='xbmc.python' version='2.14.0'/>
    <import addon="script.module.b808common" version="0.2.1"/>
  </requires>
  <extension point='xbmc.addon.metadata'>
    <platform>all</platform>
    <summary lang='en'>Installed automatically with XSqueeze - common code for XSqueeze and XSqueeze Chooser</summary>
  </extension>
  <extension point='xbmc.python.module' library='lib' />
</addon>

<addon id="script.xsqueeze"
       name="XSqueeze"
       version="1.2.7"
       provider-name="bossanova808">
  <requires>
    <import addon="xbmc.python" version="2.14.0"/>
    <import addon="script.artistslideshow" version="1.4.7"/>
    <import addon="script.module.xsqueezecommon" version="0.2.3"/>
    <import addon="script.module.b808common" version="0.2.3"/>
    <import addon="plugin.program.xsqueezechooser" version="0.2.2"/>
  </requires>
  <extension point="xbmc.python.script"
             library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <platform>all</platform>
    <summary lang="en">Squeezebox player/controller for XBMC.</summary>
    <description lang="en">Turn your XBMC into a Squeeze player or control an external Squeezebox.</description>
    <disclaimer lang="en"></disclaimer>
  </extension>
</addon>

<addon
  id="skin.confluence.bossanova808.krypton"
  version="0.0.7"
  name="Confluence Bossanova808 Krypton"
  provider-name="Jezz_X, Team Kodi, bossanova808">
  <requires>
    <import addon="xbmc.gui" version="5.12.0"/>
  </requires>
  <extension
    point="xbmc.gui.skin"
    defaultthemename="textures.xbt"
    debugging="false"
    effectslowdown="1.0">
    <res width="1280" height="720" aspect="16:9" default="true" folder="720p" />
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Confluence skin by Jezz_X modded by Bossanova808.</summary>
    <description lang="en">Bossanova808 personal mod of Confluence - just basic Confluence with a few extra nice bits and some hard coded paths - only for use by bossanova808 and friends!</description>
    <platform>all</platform>
  </extension>
</addon>

